<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tyut.mapper.FlightMapper">
    <select id="findByDepCityAndFinCity" parameterType="Flight" resultType="Flight">
        select * from flightinfo where departureCity=#{departureCity} and finalCity=#{finalCity}
    </select>
    <select id="findAllFlight" resultType="Flight">
        select * from flightinfo where 1=1
    </select>
</mapper>